{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) parsecss",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/tests/samples/build/parsecss",
			"args": ["testdata/simple1.css"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/tests/samples",
			"environment": [{"name": "LD_LIBRARY_PATH", "value": "../../src/build"}],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "build_dbg"
		},
		{
			"name": "(gdb) selectors",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/tests/selectors/build/tests",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/tests/selectors",
			"environment": [{"name": "LD_LIBRARY_PATH", "value": "../../src/build"}],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "build_dbg"
		},
		{
			"name": "(gdb) misc",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/tests/unit/out/dbg/tests",
			"args": ["--suite=misc", "--test=issue_test_1"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/tests/unit",
			"environment": [{"name": "LD_LIBRARY_PATH", "value": "../../src/out/dbg:../harness/out/dbg"}],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "build_dbg"
		},
		{
			"name": "(gdb) sample",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/tests/unit/out/dbg/tests",
			"args": ["--suite=samples", "--test=sample[7]"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/tests/unit",
			"environment": [{"name": "LD_LIBRARY_PATH", "value": "../../src/out/dbg:../harness/out/dbg"}],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "build_dbg"
		}
	]
}